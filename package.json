{
	"name": "comic-assistant",
	"version": "1.0.0",
	"description": "Google Assistant Action to view your League of Comic Geeks pull list.",
	"main": "dist/index.js",
	"private": true,
	"scripts": {
		"prebuild": "rimraf dist",
		"build": "tsc",
		"build:vercel": "ncc build src/index.ts -o api -m --target es2019",
		"lint": "eslint src --fix",
		"format": "prettier src --write"
	},
	"author": "maruf99",
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^16.7.5",
		"@typescript-eslint/eslint-plugin": "^4.29.3",
		"@typescript-eslint/parser": "^4.29.3",
		"@vercel/ncc": "^0.31.1",
		"eslint": "^7.32.0",
		"eslint-config-marine": "^9.0.6",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^3.4.1",
		"prettier": "^2.3.2",
		"rimraf": "^3.0.2",
		"typescript": "^4.4.2"
	},
	"dependencies": {
		"@assistant/conversation": "^3.8.1",
		"comicgeeks": "^1.1.0"
	}
}
