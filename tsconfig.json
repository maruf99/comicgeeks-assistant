{
	"compilerOptions": {
		"outDir": "dist",
		"lib": ["ES2021", "ESNext", "DOM"],
		"baseUrl": "./",
		"module": "CommonJS",
		"target": "ES2019",
		"strict": true,
		"alwaysStrict": true,
		"strictNullChecks": false,
		"esModuleInterop": true,
		"moduleResolution": "node",
		"removeComments": false,
		"newLine": "lf",
		"pretty": true,
		"resolveJsonModule": true,
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true,
		"importHelpers": true,
		"incremental": true
	},
	"include": ["src"]
}
